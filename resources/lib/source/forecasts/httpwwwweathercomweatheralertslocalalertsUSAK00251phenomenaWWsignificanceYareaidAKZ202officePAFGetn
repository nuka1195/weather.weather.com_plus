<script>
(function() { var loader = new YAHOO.util.YUILoader({ base:"http://s.imwx.com/js/2.8.0r4/", loadOptional:false, combine:false, filter:"MIN", allowRollup:true,
require: ["element","dom","selector","event","connection","event-mouseenter","event-delegate","datasource","autocomplete","container","containercore","cookie","animation","button","tabview","yahoo"],
onSuccess: function() {

//breadcrumb navigation
	   var bcpageId= wx.config.page.pageId;
	if (typeof(bcpageId)!='undefined' && bcpageId!="" && bcpageId!="0"){
	   var bclocId="";
	    var bcpageletURL="";
	   var bcInput="";

	   if (wx.config.loc.zip != "" && wx.config.loc.zip!="nl"){
			bclocId=wx.config.loc.zip;
		}else if (wx.config.loc.locid!=null && wx.config.loc.locid!="" && wx.config.loc.locid!="nl"  ){
			bclocId=wx.config.loc.locid.replace("$",":");

		}

	   if (typeof(wx.config.bc)=="undefined"){
	   	wx.config.bc=[];
	   }

	   if (wx.config.bc.length>0){
	  	 bcInput=wx.config.bc.join("&");
	   }
		if (bcInput==''){

		bcpageletURL=['/pagelet/bc/',bcpageId,'?',(bclocId!=""?'locid=' + bclocId:"")].join("");
		}else{
			   bcpageletURL=['/pagelet/bc/',bcpageId,'?',bcInput,"&",(bclocId!=""?'locid=' +bclocId:"")].join("");
		}





		YAHOO.util.Connect.asyncRequest('GET', bcpageletURL, {
			success:function(o) {
				if(o.status =='200') {
					YAHOO.util.Dom.get("breadcrumbDiv").innerHTML = o.responseText;
				} else {
					TWCI.BEACONS.firePageletError(bcpageletURL, bcpageId, o.status);
				}
                     	},
			failure:function(o) {
				TWCI.BEACONS.firePageletError(bcpageletURL, bcpageId, o.status);
			}
		});
	   }


