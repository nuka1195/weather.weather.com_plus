//Site Search
			       YAHOO.util.Event.on("twc-google-site-search", "submit", function(e){
					    var siteSearchForm = document.forms["twc-google-site-search"];
						var siteSearchTextBox = siteSearchForm.elements["q"];
					
						if (siteSearchTextBox.value == siteSearchTextBox.title || siteSearchTextBox.value=="") {
							YAHOO.util.Dom.addClass(siteSearchTextBox,'twc-error-state');
						} else {						
							YAHOO.weather.intelliTrak.isLinkClicked = true;
							YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'hdr_sitesearch'});
							siteSearchForm.submit();
						}		
				
						});
			     	//Web Search
			     	 YAHOO.util.Event.on("twc-google-web-search", "submit", function(e){							
							var webSearchForm = document.forms["twc-google-web-search"];
							var webSearchTextBox = webSearchForm.elements["q"];
					
							if (webSearchTextBox.value == webSearchTextBox.title || webSearchTextBox.value=="") {
								YAHOO.util.Dom.addClass(webSearchTextBox,'twc-error-state');
							} else {										
								YAHOO.weather.intelliTrak.isLinkClicked = true;
								YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'searchbox_footer_web'});	
								webSearchForm.submit();
							}				
						});

