                    var forecastModules = YAHOO.util.Selector.query(".twc-forecast-module");
                    for (var i = 0; i < forecastModules.length; i++) {
                        var forecastModule = new YAHOO.util.Element(forecastModules[i]);
                        var toggler = new YAHOO.util.Element(YAHOO.util.Selector.query('.twc-forecast-toggler', forecastModule.get('element'), true));
                        toggler.on('click', function(ev) {
                            var toggler = YAHOO.util.Selector.query('.twc-forecast-toggler', this.get('element'), true);

                            if (this.hasClass('twc-forecast-expanded')) {
                                this.removeClass('twc-forecast-expanded');
                                toggler.innerHTML = "Click for more weather info";
                            } else {
                                this.addClass('twc-forecast-expanded');
                                toggler.innerHTML = "Hide details";
                            }
                            YAHOO.util.Event.preventDefault(ev);
                        }.bind(forecastModule));
                    }

					// Add Close Window title attribute to pop-intos
					el = YAHOO.util.Dom.getElementsByClassName('container-close', 'a');
					ellength = el.length;
					for (i=0;i<ellength
