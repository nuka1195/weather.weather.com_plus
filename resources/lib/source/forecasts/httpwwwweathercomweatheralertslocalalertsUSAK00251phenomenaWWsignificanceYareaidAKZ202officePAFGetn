<!--[if lte IE 7]><style type="text/css">#alertInner {left:5px}</style><![endif]-->
	<script LANGUAGE="JavaScript1.2">
		var currframe=1;
		var framewidth=531;
		var frameheight=25;
		var imgwidth=531;
		var imgheight=25;
		var imgname='alertInnerPlaceholder';
		var divId = 'alertInner';
		var movewidth=25;
		var movespeed=200;
		var tkr_debug=false;
		var tkr_offset=(isNS6)?25:5; // was 27:33
		var tkr_MDoffset=(isNS6)?0:0; // was 0:4

		// No need for pagetype code

		function getMyX(myName) {
			return document.getElementById(myName).offsetLeft;
		}
		function getMyY(myName) {
			return document.getElementById(myName).offsetTop;
		}
		function showMLL() {
			document.getElementById("alertleft").style.visibility = "visible";
		}
		function showMRL() {
			document.getElementById("alertright").style.visibility = "visible";
		}
		function hideMLL() {
		}
		function hideMRL() {
		}
		function moveleft() {
			if (timerRunning == true) return;
			if (currframe > 1) {
				stopSlide();
				var divX = parseInt(getMyX(divId));
				var divY = parseInt(getMyY(divId));
				if (tkr_debug==true) {
					alert("moveleft - divX: "+divX+", divY: "+divY);
				}
				var previousAlertMarker = Number(getMyX('alertImg'+(currframe - 2)));
				var thisAlertMarker = Number(getMyX('alertImg'+(currframe - 1)));
				var distance = (thisAlertMarker - previousAlertMarker);
				var beginAlertMarker = tkr_offset;
				if (tkr_debug==true) {
					alert("moveleft - previousAlertMarker: "+previousAlertMarker+", thisAlertMarker: "+thisAlertMarker+", distance: "+distance+", beginAlertMarker: "+beginAlertMarker);
				}
				var eos = divX+distance;
				if (divX+distance > beginAlertMarker) eos = beginAlertMarker;
				startSlideClip(divId,eos,divY,movewidth,movespeed);
				currframe--;
				if (currframe > 1) showMLL();
				else hideMLL();
				if (currframe < alertArray.length) showMRL();
				else hideMRL();
			}
		}
		function moveright() {
			if (timerRunning == true) return;
			if (currframe < alertArray.length) {
				stopSlide();
				var divX = parseInt(getMyX(divId));
				var divY = parseInt(getMyY(divId));
				if (tkr_debug==true) {
					alert("moveright - divX: "+divX+", divY: "+divY);
				}
				var nextAlertMarker = Number(getMyX('alertImg'+currframe));
				var thisAlertMarker = Number(getMyX('alertImg'+(currframe - 1)));
				var distance = (nextAlertMarker - thisAlertMarker);
				var endAlertMarker = Number(getMyX('alertImgEnd'));
				if (tkr_debug==true) {
					alert("moveright - nextAlertMarker: "+nextAlertMarker+", thisAlertMarker: "+thisAlertMarker+", distance: "+distance+", endAlertMarker: "+endAlertMarker);
				}
				startSlideClip(divId,divX - distance,divY,movewidth,movespeed);
				currframe++;
				if (currframe > 1) showMLL();
				else hideMLL();
				if (currframe < alertArray.length && (endAlertMarker - nextAlertMarker) > framewidth) showMRL();
				else hideMRL();
			}
		}
		function startSlideClip(slideDiv,slideX,slideY,slideUnits,slideInt){
			stopSlide();
			timerRunning = true;
			xDone = false;
			yDone = false;
			var thisX = (slideX == 'null')?getMyX(slideDiv):slideX;
			var thisY = (slideY == 'null')?getMyY(slideDiv):slideY;
			slideFunc = "slideClipDiv('"+slideDiv+"',"+thisX+","+thisY+","+slideUnits+")";
			timerID = setInterval(slideFunc,slideInt);
		}
		function slideClipDiv(slideDivName,destX,destY,moveUnits){
			var yDone = false;
			var xDone = false;
			var divX = Number(getMyX(slideDivName));
			var divY = Number(getMyY(slideDivName));
			var imgX = tkr_offset;
			var imgY = 0;
			if (divX == destX || ((divX < destX)&&(divX+moveUnits >= destX)) || ((divX > destX)&&(divX-moveUnits <= destX))){
				xDone = true;
				divX = parseInt(destX);
			} else if ((divX+moveUnits) > destX) divX -= moveUnits;
			else divX += moveUnits;
			if (divY == destY || ((divY < destY)&&(divY+moveUnits >= destY)) || ((divY > destY)&&(divY-moveUnits <= destY))){
				yDone = true;
				divY = parseInt(destY);
			} else if ((divY+moveUnits) > destY) divY -= moveUnits;
			else divY += moveUnits;
			if (tkr_debug==true) {
			}
			moveDiv(slideDivName,'null',divX-tkr_MDoffset,divY);
			var tleft = 0;
			if (divX<imgX) tleft = imgX-divX;
			var tright = 0;
			if (divX<imgX) tright = imgX-divX+framewidth;
			else if (divX==imgX) tright = framewidth;
			else {
				if ((divX-imgX)<framewidth) tright = divX-imgX;
				else tright = 0;
			}
			var ttop = 0;
			if (divY<imgY) ttop = imgY-divY;
			var tbottom = 0;
			if (divY<imgY) tbottom = imgY-divY+frameheight;
			else if (divY==imgY) tbottom = frameheight;
			else {
				if ((divY-imgY)<frameheight) tbottom = divY-imgY;
				else tbottom = 0;
			}
			clipDiv(slideDivName,ttop,tright,tbottom,tleft);
			if (xDone && yDone) stopSlide();
		}

function alertObj(icon,text,link) {
	this.icon = icon;
	this.text = text;
	this.link = link;
}
var alertArray = new Array();
var iconArray = new Array();
/* Updated URL domain names for images */
iconArray['yellow'] = 'http://i.imwx.com/web/common/images/advisory_bullet.gif';
iconArray['red'] = 'http://i.imwx.com/web/common/images/warning_bullet.gif';
iconArray['orange'] = 'http://i.imwx.com/web/common/images/watch_bullet.gif';
iconArray['blank'] = 'http://i.imwx.com/web/blank.gif';




alertArray[alertArray.length] = new alertObj('blank','[State alerts for AK]','/weather/newscenter/alerts/nswxcategory/AK');

function makeLinkAndAddToDiv(divName,myLink,linkText) {
                var newTrackString = ["this.href=intelliTrak({'href':'",document.location.href,"','from':'local_alert_ticker'});"].join("");
                var oObj = document.createElement("A");
                oObj.setAttribute("href",myLink);
                oObj.setAttribute("onmousedown",setClick);
                oObj.setAttribute("onmouseup",newTrackString);
                oObj.appendChild(document.createTextNode(linkText));
                var divHandle = document.getElementById(divName);
                divHandle.appendChild(oObj);
                divHandle.appendChild(document.createTextNode("&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"));
}


function buildAlerts() {
	var txtOut = '<nobr>';
	for (var n=0;alertArray[n];n++) {
		var iconWidth = (alertArray[n].icon == 'blank')?1:13;
		txtOut += '<img src="'+iconArray[alertArray[n].icon]+'" width="'+iconWidth+'" height="13" alt="" border=0 id="alertImg'+n+'" class="alertImgIcons">&nbsp;';
        this.href =  alertArray[n].link;
    	var newTrackString ="{'href':'"+ this.href+"','from':'local_alert_ticker'}";
        txtOut += '<a href="' + alertArray[n].link + '" onmousedown="setClick();" onMouseUp="this.href=intelliTrak('+newTrackString+');">'+alertArray[n].text+'</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;';

    }
    txtOut += '<img src="'+iconArray['blank']+'" width=1 height=13 alt="" border=0 id="alertImgEnd"></nobr>';
    writeDiv('alertInner',txtOut);
    txtOut = "";
}
</script>

