<script>
// Setup Local Severe Alert Ticker


// Instantiate and configure Loader:
var loader = new YAHOO.util.YUILoader({
    base: 'http://s.imwx.com/js/2.8.0r4/',
    require: ["dom", "event", "selector", "animation"],
    loadOptional:false, 
    combine:false, 
    filter:"MIN", 
    allowRollup:true,
    onSuccess: function() {

	var loader = new YAHOO.util.YUILoader({
	    base: 'http://d.imwx.com/js/',
	    require: ["wx-jp_chunker"],
	    loadOptional:false, 
	    combine:false, 
	    //filter:"MIN", 
	    //allowRollup:true,
	    onSuccess: function() {

		try {
			var alertScroll = new YAHOO.weather.jp_chunker(
				"alertScrollBoxOuter", // could be the actual object OR the ID string
				"la-scroll-box-inner", // could be the actual object OR the ID string
				2,
				// Left Image Div Hash
				{
					'c': 'la-alert-nav-left',
					'a': 'la-textbtn-left',
					'd': 'la-textbtn-left-off'
				},
				// Right Image Div Hash
				{
					'c': 'la-alert-nav-right',
					'a': 'la-textbtn-right',
					'd': 'la-textbtn-right-off'
				},
				578, // must be an integer, not a string
				3, // must be an integer, not a string
				1.5
			).init();
		} catch (e) {
			// do nothing
		}

	    },
	    onFailure: function() {
	    }
	});

	loader.moduleInfo['wx-jp_chunker'] = {
		path: "wx-jp_chunker.js",
		type: "js"
        };
	loader.insert();

    },
    onFailure: function(){
    }
});
loader.insert();

</script>
 






   
