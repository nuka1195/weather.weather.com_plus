	        tempStr = 'http://image.weather.com/images/maps/forecast/map_wnd_day'+dayNum+'_3usc_enus_440x297.jpg';
	    }else if (getLocRegSat() == 'ec'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/map_wnd_day'+dayNum+'_3use_enus_440x297.jpg';
	    }else if (getLocRegSat() == 'wc'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/map_wnd_day'+dayNum+'_3usw_enus_440x297.jpg';
