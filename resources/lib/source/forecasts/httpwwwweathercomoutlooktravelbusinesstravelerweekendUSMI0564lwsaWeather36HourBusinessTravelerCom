<script language="JavaScript1.2">
	var highTempMapType = 'hiTempMT';
	var lowTempMapType = 'loTempMT';
	var interstateMapType = 'interstateMT';
	var golfMapType = 'golfMT';
	var windMapType = 'windMT';
	var treePollenMapType = 'treePollenMT';
	var grassPollenMapType = 'grassPollenMT';
	var ragweedPollenMapType = 'ragweedPollenMT';
	var weatherMapType = 'weatherMT';
	
	// RegSat and Offset Methods are server initialized methods
	function getLocRegSat() {
		return 'ne';
	}

	function getFridayOffset() {
		return -2;
	}

	function getSaturdayOffset() {
		return -1;
	}

	function getSundayOffset() {
		return 0;
	}
	
	var mapTypeThumbInfo = new Array();
	mapTypeThumbInfo[highTempMapType] = new Array('http://image.weather.com/web/common/maproom/hi6_76x56.jpg','High Temps');
	mapTypeThumbInfo[lowTempMapType] = new Array('http://image.weather.com/web/common/maproom/lo6_76x56.jpg','Low Temps');
	mapTypeThumbInfo[interstateMapType] = new Array('http://image.weather.com/web/common/maproom/us_interstate_natl_76x56.jpg','Driving');
	mapTypeThumbInfo[golfMapType] = new Array('http://image.weather.com/web/common/maproom/us_natl_golfindex_76x56.jpg','Golf Index');
	mapTypeThumbInfo[windMapType] = new Array('http://image.weather.com/web/common/maproom/us_wind_cur_76x56.jpg','Wind Fcst');
	mapTypeThumbInfo[treePollenMapType] = new Array('http://image.weather.com/web/common/maproom/tree_pol_76x56.jpg','Tree Pollen');
	mapTypeThumbInfo[grassPollenMapType] = new Array('http://image.weather.com/web/common/maproom/tree_pol_76x56.jpg','Grass Pollen');
	mapTypeThumbInfo[ragweedPollenMapType] = new Array('http://image.weather.com/web/common/maproom/tree_pol_76x56.jpg','Ragweed');
	mapTypeThumbInfo[weatherMapType] = new Array('http://image.weather.com/web/common/maproom/fore1_76x56.jpg','Weather');
	
	function getMapTypeThumbURL(_mapType) {
	    return mapTypeThumbInfo[_mapType][0];
	}

	function getMapTypeThumbText(_mapType) {
	    return mapTypeThumbInfo[_mapType][1];
	}


	//these functions are used for weekend maps in determining map URLs
	function getWeekendWeather440MapURL(_dayOff){
	    var tempStr = '';
	    var dayNum = Number(_dayOff+1);
	
	    if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
	            && getLocRegSat() != 'nw' && getLocRegSat() != 'sc' && getLocRegSat() != 'se' && getLocRegSat() != 'sw' && getLocRegSat() != 'wc')){
	        if (_dayOff == getFridayOffset()){
	            dayNum = 5;
	            tempStr = 'http://image.weather.com/images/maps/forecast/fore'+dayNum+'_440x297.jpg';
	        }else if (_dayOff == getSaturdayOffset()){
	            dayNum = 6;
	            tempStr = 'http://image.weather.com/images/maps/forecast/trend'+dayNum+'_440x297.jpg';
	        }else{
	            dayNum = 7;
	            tempStr = 'http://image.weather.com/images/maps/forecast/trend'+dayNum+'_440x297.jpg';
	        }
	    }
	
	    if (dayNum == 1 && getLocRegSat() == 'ec'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/map_wkpln_day'+dayNum+'_3usec_enus_440x297.jpg';
	    }else if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
	            && getLocRegSat() != 'nw' && getLocRegSat() != 'sc' && getLocRegSat() != 'se' && getLocRegSat() != 'sw' && getLocRegSat() != 'wc')){
	        if (_dayOff == getFridayOffset()){
	            dayNum = 5;
	            tempStr = 'http://image.weather.com/images/maps/forecast/fore'+dayNum+'_440x297.jpg';
	        }else if (_dayOff == getSaturdayOffset()){
	            dayNum = 6;
	            tempStr = 'http://image.weather.com/images/maps/forecast/trend'+dayNum+'_440x297.jpg';
	        }else{
	            dayNum = 7;
	            tempStr = 'http://image.weather.com/images/maps/forecast/trend'+dayNum+'_440x297.jpg';
	        }
	    }else if (getLocRegSat() == 'cn'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/map_wkpln_day'+dayNum+'_3usc_enus_440x297.jpg';
	    }else if (getLocRegSat() == 'ec'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/map_wkpln_day'+dayNum+'_3use_enus_440x297.jpg';
	    }else{
	        tempStr = 'http://image.weather.com/images/maps/forecast/map_wkpln_day'+dayNum+'_3us'+getLocRegSat()+'_enus_440x297.jpg';
	    }
	    return tempStr;
	}

	function getWeekendWind440MapURL(_dayOff){
	    var tempStr = '';
	    var dayNum = Number(_dayOff+1);
	
	    if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
