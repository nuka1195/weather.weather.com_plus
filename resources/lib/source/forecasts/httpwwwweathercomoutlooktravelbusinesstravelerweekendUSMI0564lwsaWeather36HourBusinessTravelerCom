    function isNumber(_posNum) {
        for(var i=0;i<_posNum.length; i++) {
            var nextChar = _posNum.substring(i,i+1);
            if(nextChar < "0" || nextChar > "9") {
                return false;
            }
        }
        return true;
    }

	function divDisplay(id,disValue) {
		document.getElementById(id).style.display = disValue;
	}

	function upArrow() {
		showCC();
	}
	function downArrow() {
		hideCC();
	}
	//functions for showing expanded/default view
	function hideCC() {
	    divDisplay('wefoneexpanded','block');
	    divDisplay('weftwoexpanded','block');
	    divDisplay('wefthreeexpanded','block');
	    divDisplay('wefonecollapsed','none');
	    divDisplay('weftwocollapsed','none');
	    divDisplay('wefthreecollapsed','none');
	}

	function showCC() {
		divDisplay('wefonecollapsed','block');
		divDisplay('weftwocollapsed','block');
		divDisplay('wefthreecollapsed','block');
		divDisplay('wefoneexpanded','none');
		divDisplay('weftwoexpanded','none');
		divDisplay('wefthreeexpanded','none');
	}

