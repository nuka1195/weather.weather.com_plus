	function getWeekendGolf440MapURL(_dayOff){
	    var tempStr = '';
	    var dayNum = Number(_dayOff+1);
	
	   if (dayNum == 1 || dayNum == 2){
	        if (getLocRegSat() == 'cn'){
			tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_3usc_enus_440x297.jpg';
	        }else {
	            if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
	                    && getLocRegSat() != 'nw' && getLocRegSat() != 'sc' && getLocRegSat() != 'se' && getLocRegSat() != 'sw' && getLocRegSat() != 'wc')){
	                tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_4namus_enus_440x297.jpg';
	            }else {
	                tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_3us'+getLocRegSat()+'_enus_440x297.jpg';
	            }
	        }
	
	   }else{
	        if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
	            && getLocRegSat() != 'nw' && getLocRegSat() != 'sc' && getLocRegSat() != 'se' && getLocRegSat() != 'sw' && getLocRegSat() != 'wc')){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_4namus_enus_440x297.jpg';
	        }else if (getLocRegSat() == 'cn'){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_3usc_enus_440x297.jpg';
	        }else if (getLocRegSat() == 'ec'){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_3use_enus_440x297.jpg';
	        }else if (getLocRegSat() == 'wc'){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_3usw_enus_440x297.jpg';
	        }else{
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_glfidx_day'+dayNum+'_3us'+getLocRegSat()+'_enus_440x297.jpg';
	        }
	    }
	    return tempStr;
	}
	
	function getWeekendInterstate440MapURL(_dayOff){
	    var tempStr = '';
	    var dayNum = Number(_dayOff+1);
	    var regionName = '';
	    var firstURLpart = 'http://image.weather.com/web/maps/weather/forecast/us_interstate_';
	    var lastURLpart = '_day'+dayNum+'fcst_440_en.jpg';
	
	   if (dayNum == 1 || dayNum == 2){
	        if (getLocRegSat() == 'cn'){
	            regionName = 'central';
	        }else if (getLocRegSat() == 'ec'){
	            regionName = 'eastcentral';
	        }else if (getLocRegSat() == 'nc'){
	            regionName = 'northcentral';
	        }else if (getLocRegSat() == 'ne'){
	            regionName = 'northeast';
	        }else if (getLocRegSat() == 'nw'){
	            regionName = 'northwest';
	        }else if (getLocRegSat() == 'sc'){
	            regionName = 'southcentral';
	        }else if (getLocRegSat() == 'se'){
	            regionName = 'southeast';
	        }else if (getLocRegSat() == 'sw'){
	            regionName = 'southwest';
	        }else if (getLocRegSat() == 'wc'){
	            regionName = 'westcentral';
	        }else{
	            regionName = 'national';
	        }
	        tempStr = firstURLpart+regionName+lastURLpart;
	   }else{
	        if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
	            && getLocRegSat() != 'nw' && getLocRegSat() != 'sc' && getLocRegSat() != 'se' && getLocRegSat() != 'sw' && getLocRegSat() != 'wc')){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_intrstate_day'+dayNum+'_4namus_enus_440x297.jpg';
	        }else if (getLocRegSat() == 'cn'){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_intrstate_day'+dayNum+'_3usc_enus_440x297.jpg';
	        }else if (getLocRegSat() == 'ec'){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_intrstate_day'+dayNum+'_3use_enus_440x297.jpg';
	        }else if (getLocRegSat() == 'wc'){
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_intrstate_day'+dayNum+'_3usw_enus_440x297.jpg';
	        }else{
	            tempStr = 'http://image.weather.com/images/maps/forecast/map_intrstate_day'+dayNum+'_3us'+getLocRegSat()+'_enus_440x297.jpg';
	        }
	    }
	    return tempStr;
	}
	
	function getWeekendHi440MapURL(_dayOff){
	    var tempStr = '';
	    var dayNum = 0;
	
	    if (_dayOff == getFridayOffset()){
	        dayNum = 5;
	    }else if (_dayOff == getSaturdayOffset()){
	        dayNum = 6;
	    }else{
	        dayNum = 7;
	    }
	
	    if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
	            && getLocRegSat() != 'nw' && getLocRegSat() != 'sc' && getLocRegSat() != 'se' && getLocRegSat() != 'sw' && getLocRegSat() != 'wc')){
	        tempStr = 'http://image.weather.com/images/maps/forecast/hi'+dayNum+'_440x297.jpg';
	    }else if (getLocRegSat() == 'cn'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/cen_hi'+dayNum+'_440x297.jpg';
	    }else if (getLocRegSat() == 'ec'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/ecen_hi'+dayNum+'_440x297.jpg';
	    }else if (getLocRegSat() == 'wc'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/wcen_hi'+dayNum+'_440x297.jpg';
	    }else{
	        tempStr = 'http://image.weather.com/images/maps/forecast/'+getLocRegSat()+'_hi'+dayNum+'_440x297.jpg';
	    }
	    return tempStr;
	}
	
	function getWeekendLow440MapURL(_dayOff){
	    var tempStr = '';
	    var dayNum = 0;
	
	    if (_dayOff == getFridayOffset()){
	        dayNum = 5;
	    }else if (_dayOff == getSaturdayOffset()){
	        dayNum = 6;
	    }else{
	        dayNum = 7;
	    }
	
	    if (getLocRegSat() == '' || (getLocRegSat() != '' && getLocRegSat() != 'cn' && getLocRegSat() != 'ec' && getLocRegSat() != 'nc' && getLocRegSat() != 'ne'
	            && getLocRegSat() != 'nw' && getLocRegSat() != 'sc' && getLocRegSat() != 'se' && getLocRegSat() != 'sw' && getLocRegSat() != 'wc')){
	        tempStr = 'http://image.weather.com/images/maps/forecast/lo'+dayNum+'_440x297.jpg';
	    }else if (getLocRegSat() == 'cn'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/cen_lo'+dayNum+'_440x297.jpg';
	    }else if (getLocRegSat() == 'ec'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/ecen_lo'+dayNum+'_440x297.jpg';
	    }else if (getLocRegSat() == 'wc'){
	        tempStr = 'http://image.weather.com/images/maps/forecast/wcen_lo'+dayNum+'_440x297.jpg';
	    }else{
	        tempStr = 'http://image.weather.com/images/maps/forecast/'+getLocRegSat()+'_lo'+dayNum+'_440x297.jpg';
	    }
	    return tempStr;
	}

	mDivName = 'mapWidgetDiv';
	function ww(txt) {
		document.writeln(txt);
	}
	function showMap(selectedDay,indx) {
		wkendMapTabbedWidget.selDay = selectedDay;
		wkendMapTabbedWidget.selIndex = indx;
		writeDiv(mDivName,wkendMapTabbedWidget.draw());
	}
	function _addFri(obj) {
		this.friday[this.friday.length] = obj;
	}
	function _addSat(obj) {
		this.saturday[this.saturday.length] = obj;
	}
	function _addSun(obj) {
		this.sunday[this.sunday.length] = obj;
	}
	function mapTabbedWidget() {
		this.friday = new Array();
		this.addFri = _addFri;
		this.saturday = new Array();
		this.addSat = _addSat;
		this.sunday = new Array();
		this.addSun = _addSun;
		this.draw = _writeMapHTML;
		this.getDefaultDay = _getDefaultDay;
		this.selDay = "none";
		this.selIndex = 0;
	}
	function mapDayView(thumbURL,caption,mapURL,clickURL) {
		this.thumbURL = thumbURL;
		this.caption = caption;
		this.mapURL = mapURL;
		this.clickURL = clickURL;
	}
	function _getDefaultDay() {
		if (this.friday.length > 0) return "friday";
		else if (this.saturday.length > 0) return "saturday";
		else if (this.sunday.length > 0) return "sunday";
		else return "none";

	}

function _writeMapHTML() {
	var blankImage = 'http://image.weather.com/web/blank.gif';
	var tmpString = '';

	var tdd = this.getDefaultDay();
	var tsd = this.selDay;
	var trd = (tsd == "none")?tdd:tsd;

	if (tdd == "none" || (tsd != "none" && this[tsd].length == 0)) { largemap = blankImage; largelink = "?"; }
	else if (tsd == "none") { largemap = this[tdd][this.selIndex].mapURL; largelink = this[tdd][this.selIndex].clickURL; }
	else { wkendMapTabbedWidget.selDay = tsd; largemap = this[tsd][this.selIndex].mapURL; largelink = this[tsd][this.selIndex].clickURL; }

	var selTabImg = 'http://image.weather.com/web/common/nav/map_button_on.gif';
	var unselTabImg = 'http://image.weather.com/web/common/nav/map_button_off.gif';
	var noTabImg = blankImage;
	var selClass = ' CLASS="whtVerdanaText10" ';
	var unselClass = ' CLASS="bluLink10" ';
	var noClass = '';

	if (this.friday.length == 0) { fritab = noTabImg; fricap = ''; friClass = noClass; }
	else if (trd == "friday" && this.friday.length > 0) { fritab = selTabImg; fricap = '<B>Friday</B>'; friClass = selClass; }
	else { fritab = unselTabImg; fricap = '<A HREF="javascript:showMap(\'friday\','+wkendMapTabbedWidget.selIndex+')" CLASS="bluLink10">Friday</A>'; friClass = unselClass; }

	if (this.saturday.length == 0) { sattab = noTabImg; satcap = ''; satClass = noClass; }
	else if (trd == "saturday" && this.saturday.length > 0) { sattab = selTabImg; satcap = '<B>Saturday</B>'; satClass = selClass; }
	else { sattab = unselTabImg; satcap = '<A HREF="javascript:showMap(\'saturday\','+wkendMapTabbedWidget.selIndex+')" CLASS="bluLink10">Saturday</A>'; satClass = unselClass; }

	if (this.sunday.length == 0) { suntab = noTabImg; suncap = ''; sunClass = noClass; }
	else if (trd == "sunday" && this.sunday.length > 0) { suntab = selTabImg; suncap = '<B>Sunday</B>'; sunClass = selClass; }
	else { suntab = unselTabImg; suncap = '<A HREF="javascript:showMap(\'sunday\','+wkendMapTabbedWidget.selIndex+')" CLASS="bluLink10">Sunday</A>'; sunClass = unselClass; }

	tmpString += '<TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" WIDTH="440"><TR><TD CLASS="module" VALIGN="TOP">';
	tmpString += '<TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" WIDTH="100%"><TR><TD STYLE="padding:0px;"></TD>';
	tmpString += '<TD ALIGN="RIGHT" VALIGN="MIDDLE"></TD></TR></TABLE>';
	tmpString += '<TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" CLASS="Basic" ALIGN="CENTER" WIDTH="100%"><TR><TD VALIGN="TOP"><TABLE WIDTH="100%" BORDER="0" CELLPADDING="0" CELLSPACING="0"><TR><TD COLSPAN="3" VALIGN="MIDDLE" ALIGN="CENTER">';
	tmpString += '<IMG SRC="'+largemap+'" WIDTH="440" HEIGHT="297" BORDER="0" ALT=""></TD></TR></TABLE>';
	tmpString += '<TABLE WIDTH="402" BORDER="0" CELLPADDING="0" CELLSPACING="0"><TR><TD BGCOLOR="#c0c1a4"><IMG SRC="'+blankImage+'" WIDTH="1" HEIGHT="1" BORDER="0" ALT=""></TD>';
	tmpString += '</TR>';
	tmpString += '<TR>';
	tmpString += '<TD CLASS="whtVerdanaText10" ALIGN="left">';
	tmpString += '<TABLE WIDTH="100%" BORDER="0" CELLPADDING="0" CELLSPACING="0">';
	tmpString += '<TR>';
	tmpString += '<TD STYLE="background-image:url('+fritab+'); background-repeat:no-repeat" ';
	tmpString += 'WIDTH="134" HEIGHT="18" ALIGN="CENTER"'+friClass+'VALIGN="TOP">';
	tmpString += fricap+'</TD>';
	tmpString += '<TD STYLE="background-image:url('+sattab+'); background-repeat:no-repeat" ';
	tmpString += 'WIDTH="134" HEIGHT="18" ALIGN="CENTER"'+satClass+'VALIGN="top">';
	tmpString += satcap+'</TD>';
	tmpString += '<TD STYLE="background-image:url('+suntab+'); background-repeat:no-repeat" ';
	tmpString += 'WIDTH="134" HEIGHT="18" ALIGN="CENTER"'+sunClass+'VALIGN="top">';
	tmpString += suncap+'</TD>';
	tmpString += '</TR>';
	tmpString += '<TR><TD COLSPAN="3" VALIGN="top"><IMG SRC="'+blankImage+'" WIDTH="1" HEIGHT="15" BORDER="0" ALT=""></TD></TR></TABLE>';
	tmpString += '</TD></TR>';
	tmpString += '<TR><TD BGCOLOR="#ffffff"><IMG SRC="'+blankImage+'" WIDTH="1" HEIGHT="15" BORDER="0" ALT=""></TD></TR></TABLE>';
	if (trd != "none") {
		tmpString += '<TABLE WIDTH="100%" BORDER="0" CELLPADDING="0" CELLSPACING="0"><TR>';
		for (var i = 0;i < this[trd].length; i++) {
			tmpString += '<TD CLASS="inDentA" ALIGN="left">';
			tmpString += '<FONT CLASS="blueFont10"><A ';
			tmpString += 'HREF="javascript:showMap(\''+trd+'\','+i+')"><IMG ';
			tmpString += 'SRC="'+this[trd][i].thumbURL+'" WIDTH="76" HEIGHT="56" BORDER="0" ALT=""></A><BR>';
			tmpString += '<A HREF="javascript:showMap(\''+trd+'\','+i+')">'+this[trd][i].caption+'</A></FONT></TD>';
		}
		tmpString += '</TR>';
		tmpString += '<TR><TD COLSPAN="4" BGCOLOR="#ffffff"><IMG SRC="'+blankImage+'" WIDTH="1" HEIGHT="15" BORDER="0" ALT=""></TD></TR></TABLE>';
	}
	tmpString += '</TD></TR></TABLE></TD></TR></TABLE>';
	tmpString += '<P CLASS="Spacing"></P>';
	return tmpString;
}
</script>

<SCRIPT language="JavaScript1.2">
	wkendMapTabbedWidget = new mapTabbedWidget();

    wkendMapTabbedWidget.addSun(new mapDayView(getMapTypeThumbURL(weatherMapType),getMapTypeThumbText(weatherMapType),getWeekendWeather440MapURL(0),''));
    wkendMapTabbedWidget.addSun(new mapDayView(getMapTypeThumbURL(highTempMapType),getMapTypeThumbText(highTempMapType),getWeekendHi440MapURL(0),''));
    wkendMapTabbedWidget.addSun(new mapDayView(getMapTypeThumbURL(lowTempMapType),getMapTypeThumbText(lowTempMapType),getWeekendLow440MapURL(0),''));
    wkendMapTabbedWidget.addSun(new mapDayView(getMapTypeThumbURL(interstateMapType),getMapTypeThumbText(interstateMapType),getWeekendInterstate440MapURL(0),''));

	wkendMapTabbedWidget.draw();
</SCRIPT>

