<script type="text/javascript" src="http://s.imwx.com/js/2.8.0r4/yuiloader-dom-event/yuiloader-dom-event.js"></script>
<script type="text/javascript" src="http://j.imwx.com/v.20100826.0/common/header/javascript/wx-header-events.js"></script>

<script>
	(function(){
		A21.onParseConfig.subscribe(function(){
			var Dom = TWCI.util.Dom,
			    Event = TWCI.util.Event,
				adPositions = wx.config.ad.positions,
				masterDiv = 'WX_BrandingLeaderShade',
				winShade = 'WX_WindowShade',
				leader = 'WX_Leaderboard',
				brandBar = 'WX_BrandingBar';

			Event.onContentReady(masterDiv,function(){
				var adDiv = Dom.get(masterDiv),
					newAdStyle = null;

				if (adPositions[winShade]){ 
					newAdStyle = winShade; 
					//release the height so the windowShade can allocate space dynamically
					A21.onFinish.subscribe(function(){
						Dom.setStyle(winShade + '_frame','display','block');
						Dom.setStyle(winShade + '_frame','height','0');
						Dom.setStyle(adDiv,'height','');
					});
				}
				else if (adPositions[leader]) { newAdStyle = leader; }
				else if (adPositions[brandBar]) { newAdStyle = brandBar; }

				if (null !== newAdStyle){
					// console.log("adding %o to %o",newAdStyle,adDiv);
					Dom.setAttribute(adDiv,"id",newAdStyle);
					A21.notifyRegistered(newAdStyle);
				}
			});
		});
	})();
</script>


