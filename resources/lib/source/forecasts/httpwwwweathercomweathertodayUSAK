/* Inserted by  */
try {
YAHOO.util.Get.script("http://js.revsci.net/gateway/gw.js?csid=K06578",{
  onSuccess: function(o){
    o.purge();
	var asiCountry = (typeof wx !== "undefined" && wx.config && wx.config.loc && wx.config.loc.country) || "US";
	var asiState = (typeof wx !== "undefined" && wx.config && wx.config.loc && wx.config.loc.state) || "";
	var asiDMA = (typeof wx !== "undefined" && wx.config && wx.config.loc && wx.config.loc.dma) || "";
	if (wx.config.page.pif_timeframe!="undefined" && wx.config.page.pif_timeframe=="today"){
		var asiApps = [];
		var wcua = wx.config.user.apps;
		for(var i=0,l=wcua.length;i<l;i++){
			var DFPdecl = getDeclarationDFP(wcua[i]).split(";");
			var fam = DFPdecl[0].split("=")[1];
			var name = wcua[i];
			K06578.DM_addToLoc("app_fam_"+fam,"yes");
			K06578.DM_addToLoc("app_name_"+name,"yes");
		}
	}
	
	var asiCat = "";
	
	var asiObj=new Array();
	var asiCategory="";
	if (wx.config.metrics!="undefined"){
		asiCat=wx.config.metrics.level1;
		if (wx.config.metrics.level2!=""){
			asiCat=asiCat+"-"+wx.config.metrics.level2;
		}
		if (wx.config.metrics.level3!=""){
			asiCat=asiCat+"-"+wx.config.metrics.level3;
		}
		if (wx.config.metrics.level4!=""){
			asiCat=asiCat+"-"+wx.config.metrics.level4;
		}
		
	}
	if(asiCat != undefined && asiCat != ""){
		var asiStr=asiCat;var asiSplitUP = asiStr.split("-");
		for (var i=0; i < asiSplitUP.length; i++){
			asiObj[i] = asiSplitUP[i];
		}
		asiCategory = asiObj.join(" > ");asiCategory = asiCategory+" > ";
		K06578.DM_addToLoc("beh_cat",asiCat);
		K06578.DM_cat(asiCategory);
	}
	
	K06578.DM_addToLoc("country",asiCountry);
	K06578.DM_addToLoc("state",asiState);
	K06578.DM_addToLoc("dma",asiDMA);
	
	K06578.DM_tag();
  },
  onFailure: function(o){
    alert("Failed to load audience science");
  }
});
} catch (err) { try { console.log(err); } catch(error) {} }
