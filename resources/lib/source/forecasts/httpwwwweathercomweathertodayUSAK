	function getUPSlot(slotNo){
        var slotValue = Cookie.get("UserPreferences",function(str){
        	if(str.indexOf("%7C")>0) {
        		return str.split("%7C")[slotNo];  
        	} else {
          		return str.split("|")[slotNo];
          	}
        });
        return slotValue
      }
	
	function getDeclaration(){
        var declaration = "Undeclared";
        var oldDecl = getUPSlot(16);
          
        if(appsInfo != null && appsInfo.length>0){
        	//alert("Got 1");
           declaration = appsInfo[0];
        }else if(uInfo === "0^noId" && oldDecl !== "Undeclared"){
          // alert("Got 2");
           var oldToNew = {"Allergies":"pollen",
