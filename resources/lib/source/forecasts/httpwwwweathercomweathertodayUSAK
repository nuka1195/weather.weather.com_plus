try {
var Selector = YAHOO.util.Selector;
var Connect = YAHOO.util.Connect;
var Dom = YAHOO.util.Dom;
var appsList = wx.config.user.apps;
var PATH="/pagelet/apps/";
// Setup apps
var apps =YAHOO.weather.Apps = new YAHOO.weather.AppsConstructor("USAK0025:1:US",appsList);

// Setup app selector

YAHOO.util.Event.onAvailable("twc-app-select-filters-id",function(){
	YAHOO.util.Event.delegate("twc-app-select-filters-id", "click", function(e, matchedEl, container){ 
	  var thisapp = YAHOO.weather.Apps;
	  YAHOO.util.Dom.addClass(matchedEl.parentNode, 'twc-active'); 
	  if (thisapp.SELECTED_LINK !== null){
	    YAHOO.util.Dom.removeClass(thisapp.SELECTED_LINK, 'twc-active');  
	  }
	  
	  //all apps is the default filter, remove the active after someone clicks
	  if(thisapp.INITIAL_FILTER_SELECTED == false)
	  { 
	    YAHOO.util.Dom.removeClass("allappsfilter", 'twc-active');  
	    thisapp.INITIAL_FILTER_SELECTED = true;
	  }
	  
	  thisapp.SELECTED_LINK = matchedEl.parentNode;
	  var filter = matchedEl.getAttribute("filter");
	  filter += "&i="+wx.config.user.signature;
	  YAHOO.util.Connect.asyncRequest('GET', filter , {
		    success:function(o) {
		   	  if(o.status.toString().match("2[0-9][0-9]")!=null) {
				     	 // This member is called by handleSuccess
				      var appListContainer = document.getElementById("twc-app-select-list-id");
				      appListContainer.innerHTML = o.responseText;
				      var appLIs = Selector.query("li","twc-app-select-list-id");
				      var appsOnPage = Selector.query(".twc-app-holder","twc-apps-module");
				      var selectedApps = [];
				      for(var i = 0;i < appsOnPage.length;i++){
				      	selectedApps.push(appsOnPage[i].id);
				      }
				      if(appLIs.length===0) {
				      	  var errDiv = document.createElement("div");
				      	  Dom.addClass(errDiv,"twc-app-error-msg");
				      	  var txt = document.createTextNode("You have not added any apps. Please select a category from the list at left to view and add available apps.");
				      	  errDiv.appendChild(txt);
				      	  appListContainer.appendChild(errDiv);
				      }
					for(var i = 0;i < appLIs.length;i++){
						var appName = appLIs[i].className.match(/twc-app-(.[^ ]+)/)[1];
						var appAdd = Selector.query("span.twc-headline-add",appLIs[i],true);
						var appDelete = Selector.query("span.twc-headline-delete",appLIs[i],true);
						if(thisapp._appIndex(appName,selectedApps)>0){
							Dom.addClass(appDelete,"yui-hidden");
							Dom.removeClass(appAdd,"yui-hidden");
						}
						else
						{
							Dom.addClass(appAdd,"yui-hidden");
							Dom.removeClass(appDelete,"yui-hidden");
						}
					} 	 
	     	} else {
	     		TWCI.firePageletError(wx.config.page.pageId,"app",o.status);
	     	}
	    
	    },
	    failure:function(o) {
	      // Failure handler
	      TWCI.firePageletError(wx.config.page.pageId,"app",o.status);
	    },
	    scope: this,
	    cache:false
	  });   
	  YAHOO.util.Event.stopEvent(e);
	}, "span.twc-app-filters-category");
});

//var ifChangedLocation = apps.resolvedLocationApp(wx.config.page);
} catch (err) { try { console.log(err); } catch(error) {} }

