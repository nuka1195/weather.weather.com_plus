function getDeclaration(state){
switch(state) {
	case"AchesAndPains":	return "hlth_pain";break;
	case"Allergies":		return "hlth_algy";break;
	case"AirQuality":		return "hlth_brth";break;
	case"ColdAndFlu":		return "hlth_flu";break;
	case"SunSafety":		return "hlth_skin";break;
	case"Fitness":			return "hlth_fit";break;
	case"Health":			return "hlth_genhlth";break;
	case"BusinessTravel":	return "trvl_btrav";break;
	case"VacationPlanner":	return "trvl_vac";break;
	case"Driving":			return "drv_intst";break;
	case"SportingEvents":	return "sprts_evnts";break;
	case"Weddings":			return "hnf_wed";break;
	case"Golf":				return "sprts_golf";break;
	case"Ski":				return "sprts_ski";break;
	case"BoatAndBeach":		return "outd_boat";break;
	case"Outdoors":			return "outd_outd";break;
	case"HomePlanner":		return "hnf_home";break;
	case"Lawn":				return "hnf_gard";break;
	case"Pets":				return "hnf_pets";break;
	case"Schoolday":		return "hnf_schdy";break;
	case "LivingSmart":		return "clim_green";break;
	//family declarations
	case "Travel": 			return "trvl_none";break;
	case "HealthyLiving" : 	return "hlth_none";break;
	case "OutdoorActivity": return "outd_none";break;
	case "Sports":			return "sprts_none";break;
	case "Climate":			return "clim_none";break;
	case "Home":			return "hnf_none";break;
	default:				return "undcl";
	}
}
