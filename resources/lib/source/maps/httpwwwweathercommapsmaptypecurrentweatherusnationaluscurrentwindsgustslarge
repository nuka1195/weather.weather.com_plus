function getImapDec(state) {
	switch(state) {
			case "Undeclared":return"undcl_maps";
			case "AchesAndPains":return "hlth_pain_maps";
			case "Allergies":return "hlth_algy_maps";
			case "AirQuality":return "hlth_brth_maps";
			case "ColdAndFlu":return "hlth_flu_maps";
			case "SunSafety":return "hlth_skin_maps";
			case "Fitness":return "hlth_fit_maps";
			case "Health":return "hlth_genhlth_maps";
			case "BusinessTravel":return "trvl_btrav_maps";
			case "VacationPlanner":return "trvl_vac_maps";
			case "Driving":return "drv_intst_maps";
			case "SportingEvents":return "evt_sport_maps";
			case "Weddings":return "evt_wed_maps";
			case "Golf":return "recr_golf_maps";
			case "Ski":return "recr_ski_maps";
			case "BoatAndBeach":return "recr_boat_maps";
			case "Outdoors":return "recr_outd_maps";
			case "HomePlanner":return "hng_home_maps";
			case "Lawn":return "hng_gard_maps";
			case "Pets":return "hng_pets_maps";
			case "Schoolday":return "hng_schdy_maps";
			default:return"undcl_maps";
		}
}



