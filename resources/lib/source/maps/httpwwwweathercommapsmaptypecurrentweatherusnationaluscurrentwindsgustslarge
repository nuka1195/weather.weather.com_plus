<script language="JavaScript1.2">
	var URLKey = new Object;
	function putURLKey(URLVariable, URLValue){
		URLKey[URLVariable] = URLValue;
	}
	function getURLVariable(URLVariable){
		return unescape(URLKey[URLVariable]);
	}

	var is_input = document.URL.indexOf('?');
	var addr_str = "";
	if (is_input != -1)
	addr_str = document.URL.substring(is_input+1, document.URL.length);
	if (addr_str){
		uriRaw = addr_str.split("&");
		for (i=0; i<uriRaw.length; i++){
			VariableKeySet = uriRaw[i].split("=");
			URLVariable = VariableKeySet[0];
			URLValue = VariableKeySet[1];
			putURLKey(URLVariable,URLValue);
		}
	}
	var clip = getURLVariable("clip");
	var region = getURLVariable("region");
	var collection = "localwxforecast";
	var presname = getURLVariable("presname");
	var clipinfo = "";

	if (clip == "null" || clip == "undefined" || clip == "" || clip.length < 1) {
		if (region == "ne") {
			clip = "756";
			collection = "regwxforecast";
			presname = "Northeast Forecast";
		} else if (region == "we") {
			clip = "363";
			collection = "regwxforecast";
			presname = "Western Forecast";
		} else if (region == "mw") {
			clip = "360";
			collection = "regwxforecast";
			presname = "Midwest Forecast";
		} else if (region == "se") {
			clip = "362";
			collection = "regwxforecast";
			presname = "Southern Forecast";
		}



	}
	clipinfo = "?clip=" + clip + "&region=" + getURLVariable("region") + "&collection=" + collection + "&presname=" + presname;

</script>
