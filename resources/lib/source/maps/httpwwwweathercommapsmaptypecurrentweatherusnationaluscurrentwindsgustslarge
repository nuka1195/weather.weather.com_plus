<script type="text/javascript">



function doHeadSpon() {
	if(document.getElementById('headSpon') ) {
		var ed=new Date(2008,3,22,23,59,59);
		var td=new Date();
                var fd=new Date(2008,3,25);
		if (td.getTime() <= ed.getTime()){
			document.getElementById('headSpon').innerHTML = "Earth Day is April 22";
		}else if(td.getTime()<fd.getTime()){
			document.getElementById('headSpon').innerHTML = "Earth Day is Every Day";
		}
	}
}
if(pageName == "/index.html" || pageName =="/" || pageName =="") { 
	//window.onload = doHeadSpon;
}



	if(param('DFPdebug')) {
    (function() {
        var positions = wx.config.ad.sequence;
        for (var i = 0; i < positions.length; i++) {
            A21.getPosReqDetails(positions[i]);
        }
    })();
}

</script>








