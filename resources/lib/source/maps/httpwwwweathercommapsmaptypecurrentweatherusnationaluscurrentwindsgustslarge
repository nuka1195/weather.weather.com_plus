function clickLocator() {
	var new_url=document.mapCollection.collection.options[document.mapCollection.collection.selectedIndex].value;

	var temp_url = "/maps/activity/";
	if (new_url == "currentweatherusnational" || new_url == "dopplerradarusnational" || new_url == "earthquakereports"
	|| new_url == "tendayforecastusnational" || new_url == "satelliteusnational" || new_url == "satelliteworld"
	|| new_url == "severeusregional" || new_url == "forecastsusnational" || new_url == "weeklyplannerusnational" || new_url == "severeusnational"
	|| new_url == "currentweatherusregional" || new_url == "forecastsusregional"){
	temp_url = "/maps/maptype/";
	temp_url += new_url += "/index_large.html";
	}else if (new_url == "oceans" || new_url == "hawaiius" || new_url == "centralus" || new_url == "eastcentralus"
	|| new_url == "midwestus" || new_url == "northcentralus" || new_url == "northeastus" || new_url == "northwestus"
	|| new_url == "southcentralus" || new_url == "southeastus" || new_url == "southwestus" || new_url == "westus"
	|| new_url == "westcentralus" || new_url == "asia" || new_url == "australia" || new_url == "centralamerica"
	|| new_url == "europe" || new_url == "northamerica" || new_url == "pacific" || new_url == "southamerica"
	|| new_url == "africaandmiddleeast" || new_url == "alaskaus" || new_url == "polar"){
	temp_url = "/maps/geography/";
	temp_url += new_url += "/index_large.html";
	}else if(new_url == "winter"){
	temp_url='/maps/activity/weddings/winter/index_large.html';
	}else if(new_url == "summer"){
	temp_url='/maps/activity/weddings/summer/index_large.html';
	}else if(new_url == "fall"){
	temp_url='/maps/activity/weddings/fall/index_large.html';
	}else if(new_url == "spring"){
	temp_url='/maps/activity/weddings/spring/index_large.html';
	}else{
	temp_url += new_url += "/index_large.html";
	}
	document.location=temp_url;
}

function popLocator() {
	// find the value of the checked radio button
	var checkedButton = 0;
	for (var i=0;i<document.mapCollection.map.length;i++) {
		if (document.mapCollection.map[i].checked==true)
			checkedButton=document.mapCollection.map[i].value;
	}
   	// empty out the selector first
	for (a=document.mapCollection.collection.options.length-1;a>0;a--){
		document.mapCollection.collection.options[a]=null;
	}
	// fill in the selector with the values in the 2D array where the
	// key of the 1st D is the value of the checked radio button
	var mapCounter = 0;
	for (a in mapTypes[checkedButton]){
		document.mapCollection.collection.options[mapCounter]=new Option(mapTypes[checkedButton][a],a);
		mapCounter++;
	}
	var myPath = location.pathname;
	for (var c=0;c<document.mapCollection.collection.options.length;c++) {
		if (myPath.indexOf(document.mapCollection.collection.options[c].value) >= 1) {
			if(myPath == "/maps/activity/fallfoliage/index_large.html"){
				document.mapCollection.collection.selectedIndex=7;
			}else{
				document.mapCollection.collection.selectedIndex=c;
			}
		}
	}
mapTitleList = new Array();
mapTitleList['outdooractivity'] = 'Select an Outdoor Activity Map Collection';
mapTitleList['healthandsafety'] = 'Select a Health and Safety Map Collection';
mapTitleList['weatherdetails'] = 'Select a Weather Details Map Collection';
writeDiv('collectionTitle',mapTitleList[checkedButton]);
}
function initLocator() {
	var myPath = location.pathname;
	for (var a in mapTypes) {
		for (var b in mapTypes[a]) {
			if (myPath.indexOf(b) >= 1 && myPath.indexOf("/outlook/") == -1) {
				// click the correct radio button
				for (var i=0;i<document.mapCollection.map.length;i++) {
					if (document.mapCollection.map[i].value==a) {
						document.mapCollection.map[i].click();
					}

				}
				popLocator();
			}
		}
	}
}
addEvent(window,'load',initLocator);
</script>
