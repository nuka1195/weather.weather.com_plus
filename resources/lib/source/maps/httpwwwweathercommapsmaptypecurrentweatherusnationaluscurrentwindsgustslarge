	<script type="text/JavaScript">
		var provCount = 1;
		provCount = provCount - 1;
		var provider_alt = "quigo";
		
		var pa_percent = "";	
		var percent_int = parseInt(pa_percent, 10);	
		var second_per_ten_percent = 6;			
		
		prov_endsec = prov_startsec + (percent_int * second_per_ten_percent) / 10;
		
		
		var pa_starttime = "";
		var pa_endtime = "";
		providerArr[provCount] = new Array(provider_alt, prov_startsec, prov_endsec, pa_starttime, pa_endtime);
		
		prov_startsec = prov_endsec;
	</script>



	<script type="text/JavaScript">	
		var server_second = 57;							
		for (var i = 0; i < providerArr.length; i ++ ) {		
			if (server_second >= providerArr[i][1] && server_second < providerArr[i][2]) {
				var startsec = providerArr[i][1];
				var endsec = providerArr[i][2];
				provider = providerArr[i][0];
				break;
			} 			
		}
