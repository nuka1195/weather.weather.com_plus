						if (fromString=="searchbox_typeahead" || fromString=="hdr_localsearch" ||fromString=="enhsearch" || fromString=="enhsearch_didyoumean" || fromString=="enhsearch_drilldown") {
							//update saved locations
							YAHOO.util.Connect.asyncRequest('POST', '/pagelet/loc?pad=:', {
							success:function(o) {
if(o.status =='200'){							YAHOO.util.Dom.get("save-locations").innerHTML = o.responseText;
}							
 },
							failure:function(o) {
								//alert("save failure");
							}
							});
						}
					}
					if (fromString=="hdr_trptsearch") {
