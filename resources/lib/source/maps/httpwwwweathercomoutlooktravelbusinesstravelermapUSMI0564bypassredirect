try {
(function() { 
    var Y = TWCI.util,        
        Dom = Y.Dom; 
    
        YUE = TWCI.util.Event;
        
      
       
        
        YUE.addListener(Dom.get('mapSelect'), "change", function(){
        	var mapdest=(Dom.get('mapSelect').options[Dom.get('mapSelect').selectedIndex].value);
		
				
			
			window.location.href="/weather/map/USMI0564?mapdest=" + mapdest;
        });
        
        var helpPanel = new YAHOO.widget.Panel(
		"twc-help-pop-1", {
			close:true,
			constraintoviewport: true,
			fixedcenter: true,
			modal: true,
			underlay: "none",
			visible: false,
			zIndex: 16777271

 		}
		);	
 	
	helpPanel.render();
	
	YUE.addListener(Dom.get('mapHelp'), "click", function(e){ 
	
		YUE.stopEvent(e);
        var target = YUE.getTarget(e);
		
		var uri = target.getAttribute('rel');
		
		
		Y.Connect.asyncRequest('GET', '/pagelet/help/' +uri, { 
		  success: function(postBack){
			helpPanel.setBody(postBack.responseText);
			helpPanel.show();
			sc_trackAction(this,"classicmap_help_doppler-radar-600-mile",s_account);
		  }, 
		  failure: function(postBack){
			
		  }
		});
		
	 });
	
		
        
	
})();
} catch (err) { try { console.log(err); } catch(error) {} }

