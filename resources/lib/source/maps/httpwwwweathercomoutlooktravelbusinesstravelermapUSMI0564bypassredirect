YAHOO.namespace("iesix.container");

		function initIE6() {
			 // Move panel to body
			 (function(){
			    var Y = YAHOO.util,
			      YUD = Y.Dom,
			      YUE = Y.Event;

			    YUE.onDOMReady(function(){
			        var ph = YUD.get('overlay1'),
			             b = document.body;
			        b.appendChild(ph);
			    });
			})();

			// Build overlay1 based on markup, initially hidden, fixed to the center of the viewport, and 300px wide
			YAHOO.iesix.container.overlay1 = new YAHOO.widget.Overlay("overlay1", { fixedcenter:true,
													visible:true,
				         								  width:"392px",
				         								  zIndex:999999 } );
			YAHOO.iesix.container.overlay1.render();
			YAHOO.util.Event.addListener("show1", "click", YAHOO.iesix.container.overlay1.show, YAHOO.iesix.container.overlay1, true);
			YAHOO.util.Event.addListener("twc-clickable", "click", YAHOO.iesix.container.overlay1.hide, YAHOO.iesix.container.overlay1, true);
			YAHOO.util.Event.addListener("container-close", "click", YAHOO.iesix.container.overlay1.hide, YAHOO.iesix.container.overlay1, true);


		}

		if(wx.config.page.fv==1) {
			YAHOO.util.Event.addListener(window, "load", initIE6);
		}

