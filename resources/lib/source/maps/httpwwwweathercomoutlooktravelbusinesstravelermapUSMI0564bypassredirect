loader.moduleInfo['wx-pageoptions'] = { path:"wx-pageoptions.js", type:"js"};
loader.moduleInfo['wx-survey'] = { path:"wx-survey.js", type:"js"};
loader.insert();
}});
loader.insert();
})();
