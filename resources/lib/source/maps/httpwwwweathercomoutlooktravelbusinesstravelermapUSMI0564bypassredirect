function addCloseicon(closeIcon){
// Added Close Window title attribute to pop-intos
	el = YAHOO.util.Dom.getElementsByClassName(closeIcon, 'a');
	ellength = el.length;
	for (i=0;i<ellength;i++) {
		el[i].setAttribute('title', 'Close Window');
	}
}
