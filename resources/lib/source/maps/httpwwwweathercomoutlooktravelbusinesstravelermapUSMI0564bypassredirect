<script type="text/javascript">
(function() { var loader = new YAHOO.util.YUILoader({ base:"http://s.imwx.com/js/2.8.0r4/", loadOptional:false, combine:false, filter:"MIN", allowRollup:true,
require: ["animation","button","connection","container","containercore","cookie","dom","event-mouseenter","event-delegate","selector","tabview","yahoo","datasource","autocomplete","element","event"], onSuccess: function() {

/* Inserted by wx-bind */
try {
YAHOO.namespace("weather");
} catch (err) { try { console.log(err); } catch(error) {} }

/* Inserted by wx-savedlocations */
try {
// Setup tab views
            var tabContainers = YAHOO.util.Selector.query(".twc-tabview");
            for (var i = 0; i < tabContainers.length; i++) {
                new YAHOO.widget.TabView(tabContainers[i]);
            }
            var body = new YAHOO.util.Element(YAHOO.util.Selector.query('body')[0]);
            body.addClass("has-js");
            body.removeClass("no-js");

			(function(){
				var Y = YAHOO.util,
				  YUD = Y.Dom,
				  YUE = Y.Event;

/* Moved header.jspf include to direct descendant of body, so this code is no longer necessary */
/*
				YUE.onDOMReady(function(){
					var ph = YUD.get('wx-header-ph'),
						wr = YUD.get('wx-header-wrapper'),
						 b = document.body;
					b.insertBefore(wr,b.firstChild);
					YUD.setY(wr, YUD.getY(ph));
				});
*/
			})();
} catch (err) { try { console.log(err); } catch(error) {} }

/* Inserted by wx-weathersearch */
try {
//TODO - move this code to a seperate file

			// Create the fahrenheight toggler
			var oButtonGroup1 = new YAHOO.widget.ButtonGroup("wx-temperature-toggle");
			var temperatureToggle = YAHOO.util.Dom.get("wx-temperature-toggle");
			var unwantedLabels = YAHOO.util.Selector.query('#wx-temperature-toggle label');

			// Remove the unwanted labels left over by YUI
			for (var i = 0; i < unwantedLabels.length; i++) {
			    temperatureToggle.removeChild(unwantedLabels[i]);
			}

			//Add event to temperature toggles
			YAHOO.util.Event.delegate("wx-temperature-toggle","click",function(e, matchedEl, container){
				YAHOO.util.Event.stopEvent(e);
				var unitPref = matchedEl.innerHTML;
				// Added for addition tracking since YUI strips from button tag.
				YAHOO.weather.intelliTrak.isLinkClicked = true;
				if(typeof wx.config.page != "undefined") {
					if(unitPref=='m')
						YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'hp_unitC'});
					else
						YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'hp_unitF'});

				}
				YAHOO.util.Connect.asyncRequest('POST', '/pagelet/userprefset?u='+unitPref, {
				success:function(o) {
					if(o.status.toString().match("2[0-9][0-9]")!=null) {
						window.location.reload();
					} else {
							TWCI.firePageletError(wx.config.page.pageId,"userPref",o.status);
					}
				},
				failure:function(o) {
					TWCI.firePageletError(wx.config.page.pageId,"app",o.status);
				}
				});
			},"button");

			//TODO - Move to separate file
			var ursId = wx.config.user.ursId;
			var products = wx.config.user.products.join(",");
			var signedIn = wx.config.user.signedIn;
			if (signedIn != null ) {
				var wxMyPageLi = YAHOO.util.Selector.query(".wx-signin-mypage");
				var wxGoldLi = YAHOO.util.Selector.query(".wx-signin-gold");
				var wxNotifyLi = YAHOO.util.Selector.query(".wx-signin-notify");
				var wxMyProfileLi = YAHOO.util.Selector.query(".wx-signin-myprofile");
				var wxSignOutLi = YAHOO.util.Selector.query(".wx-signin-signout");
				var wxRegisterLi = YAHOO.util.Selector.query(".wx-signin-register");
				var wxSignInLi = YAHOO.util.Dom.get("wx-header-tools-sign-in");

				YAHOO.util.Dom.addClass(wxSignInLi, "twc-hide");
				YAHOO.util.Dom.addClass(wxRegisterLi, "twc-hide");
				YAHOO.util.Dom.removeClass(wxSignOutLi, "twc-hide");
				YAHOO.util.Dom.removeClass(wxMyProfileLi, "twc-hide");
				if (products.indexOf("MPP") != -1) {
					YAHOO.util.Dom.removeClass(wxMyPageLi, "twc-hide");
				}
				if (products.indexOf("MWG") != -1) {
					YAHOO.util.Dom.removeClass(wxGoldLi, "twc-hide");
				}
				if (products.indexOf("SA") != -1) {
					YAHOO.util.Dom.removeClass(wxNotifyLi, "twc-hide");
				}
			}
} catch (err) { try { console.log(err); } catch(error) {} }

/* Inserted by $empty1 */
try {
YAHOO.util.Event.on("twc-google-site-search", "submit", function(e){
					    var siteSearchForm = document.forms["twc-google-site-search"];
						var siteSearchTextBox = siteSearchForm.elements["q"];

						if (siteSearchTextBox.value == siteSearchTextBox.title || siteSearchTextBox.value=="") {
							YAHOO.util.Dom.addClass(siteSearchTextBox,'twc-error-state');
						} else {
							YAHOO.weather.intelliTrak.isLinkClicked = true;
							YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'hdr_sitesearch'});
							siteSearchForm.submit();
						}

						});
} catch (err) { try { console.log(err); } catch(error) {} }

